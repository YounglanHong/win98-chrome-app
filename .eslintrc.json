{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-new": "off",
    "no-console": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off"
  }
}
